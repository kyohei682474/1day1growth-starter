// prisma/schema.prisma


datasource db {
provider = "sqlite"
url = env("DATABASE_URL")
}


generator client {
provider = "prisma-client-js"
}




model GrowthEntry {
id String @id @default(cuid())
date DateTime @default(now())
text String
effort Int // 1..5
tags String // JSON文字列として保存
createdAt DateTime @default(now())
updatedAt DateTime @updatedAt


@@index([date])
}